{
  "name": "vuesion",
  "private": true,
  "version": "4.3.0",
  "description": "The most complete boilerplate for production-ready PWAs. With focus on performance, development speed, and best practices",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "jest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "NODE_ICU_DATA=node_modules/full-icu nuxt -o",
    "storybook:dev": "vuesion storybook --dev -s ./src/static",
    "test": "NODE_ICU_DATA=node_modules/full-icu vuesion test --coverage",
    "generate": "vuesion generate",
    "type-gen:gql": "graphql-codegen",
    "type-gen:swagger": "openapi-typescript https://petstore.swagger.io/v2/swagger.json --output src/interfaces/swagger.ts",
    "extract-i18n-messages": "vuesion em -s -u -t",
    "test:watch": "NODE_ICU_DATA=node_modules/full-icu vuesion test --coverage --watch",
    "e2e": "vuesion e2e",
    "lint": "vuesion lint --cache",
    "clean": "vuesion clean",
    "storybook:build": "vuesion storybook",
    "update": "vuesion update",
    "prettier": "vuesion prettier",
    "release:major": "vuesion release --major",
    "release:minor": "vuesion release --minor",
    "release:patch": "vuesion release --patch",
    "build": "nuxt build --modern",
    "build:analyze": "nuxt build --analyze",
    "build:spa": "nuxt generate --modern",
    "start": "NODE_ICU_DATA=node_modules/full-icu nuxt start",
    "e2e:ci": "npx start-server-and-test dev http://localhost:3000 e2e",
    "ci": "vuesion parallel \"npm run lint\" \"npm run test -- --runInBand\" && npm run e2e:ci",
    "postci": "vuesion parallel \"npm run build\" \"npm run storybook:build\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "3.3.6",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/schema-ast": "1.18.1",
    "@graphql-codegen/typescript": "1.20.2",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@nuxtjs/apollo": "4.0.1-rc.5",
    "@nuxtjs/auth-next": "5.0.0-1613647907.37b1156",
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/composition-api": "0.20.2",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/robots": "2.5.0",
    "@vue/apollo-composable": "4.0.0-alpha.12",
    "@vuesion/models": "4.0.0-beta.3",
    "@vuesion/utils": "4.0.0-beta.3",
    "@vuesion/vuex-persist": "4.0.0-beta.3",
    "animejs": "3.2.1",
    "apollo-cache-inmemory": "1.6.6",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "form-data": "2.3.3",
    "full-icu": "1.3.4",
    "graphql": "15.4.0",
    "helmet": "4.4.1",
    "lodash": "4.17.21",
    "marked": "2.0.1",
    "node-fetch": "2.6.1",
    "nuxt": "2.15.2",
    "nuxt-i18n": "6.20.3",
    "nuxt-winston-log": "1.2.0",
    "openapi-typescript": "3.2.3",
    "react": "16.14.0",
    "vee-validate": "3.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@nuxt/types": "2.15.2",
    "@nuxt/typescript-build": "2.0.6",
    "@nuxtjs/color-mode": "2.0.5",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/html-validator": "0.4.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/user-event": "13.1.9",
    "@testing-library/vue": "5.6.1",
    "@types/animejs": "3.1.2",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/marked": "1.2.2",
    "@types/testing-library__jest-dom": "5.9.5",
    "@vue/test-utils": "1.1.3",
    "@vuesion/service": "4.0.0-beta.3",
    "@vuesion/storybook-theme-switcher": "4.0.0-beta.3",
    "axios-mock-adapter": "1.19.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "flush-promises": "1.0.2",
    "husky": "5.1.1",
    "identity-obj-proxy": "3.0.0",
    "mock-apollo-client": "1.1.0",
    "node-sass": "5.0.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  }
}
