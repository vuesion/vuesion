{
  "name": "vuesion",
  "private": true,
  "version": "3.7.2",
  "description": "The most complete boilerplate for production-ready PWAs. With focus on performance, development speed, and best practices",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "jest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "nuxt -o",
    "generate": "vuesion generate",
    "extract-i18n-messages": "vuesion em -s -u -t",
    "test": "vuesion test --coverage",
    "test:watch": "vuesion test --coverage --watch",
    "e2e": "vuesion e2e",
    "lint": "vuesion lint --cache",
    "clean": "vuesion clean",
    "storybook:dev": "vuesion storybook --dev",
    "storybook:build": "vuesion storybook",
    "update": "vuesion update",
    "prettier": "vuesion prettier",
    "release:major": "vuesion release --major",
    "release:minor": "vuesion release --minor",
    "release:patch": "vuesion release --patch",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "build:spa": "nuxt generate",
    "start": "nuxt start",
    "e2e:ci": "npx start-server-and-test dev http://localhost:3000 e2e",
    "ci": "vuesion parallel \"npm run lint\" \"npm run test -- --runInBand\" \"npm run e2e:ci\"",
    "postci": "vuesion parallel \"npm run build\" \"npm run storybook:build\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@nuxtjs/auth-next": "5.0.0-1613647907.37b1156",
    "@nuxtjs/axios": "5.13.1",
    "@nuxtjs/composition-api": "0.20.2",
    "@nuxtjs/pwa": "3.3.5",
    "@vuesion/models": "4.0.0-beta.3",
    "@vuesion/utils": "4.0.0-beta.3",
    "@vuesion/vuex-persist": "4.0.0-beta.3",
    "animejs": "3.2.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "helmet": "4.4.1",
    "lodash": "4.17.21",
    "marked": "2.0.1",
    "nuxt": "2.15.2",
    "nuxt-i18n": "6.20.3",
    "vee-validate": "3.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@nuxt/types": "2.15.2",
    "@nuxt/typescript-build": "2.0.6",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/vue": "5.6.1",
    "@types/animejs": "3.1.2",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/marked": "1.2.2",
    "@types/testing-library__jest-dom": "5.9.5",
    "@vue/test-utils": "1.1.3",
    "@vuesion/service": "4.0.0-beta.3",
    "@vuesion/storybook-theme-switcher": "4.0.0-beta.3",
    "axios-mock-adapter": "1.19.0",
    "babel-core": "7.0.0-bridge.0",
    "eslint-plugin-nuxt": ">=2.0.0",
    "flush-promises": "1.0.2",
    "husky": "5.1.1",
    "identity-obj-proxy": "3.0.0",
    "node-sass": "5.0.0"
  },
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 6.0.0"
  },
  "jest": {
    "globals": {
      "window": true,
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      },
      "vue-jest": {
        "experimentalCSSCompile": false
      }
    },
    "rootDir": ".",
    "setupFiles": [
      "<rootDir>/src/test/jestsetup.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/jest-dom-importer.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^vue$": "vue/dist/vue.common.js",
      "@nuxtjs/composition-api": "@nuxtjs/composition-api/lib/entrypoint.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "/cypress/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*",
      "!src/**/*.stories.ts",
      "!src/**/*.d.ts"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/api/",
      "/assets/",
      "/enums/",
      "/fixtures/",
      "/interfaces/",
      "/layouts/",
      "/plugins/",
      "/services/",
      "/static/",
      "/test/",
      "src/store/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "preset": "ts-jest",
    "testMatch": null
  }
}
